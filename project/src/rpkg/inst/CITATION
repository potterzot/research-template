citHeader("To cite this library in publications use:")

citEntry(entry = "Article",
  title        = "Article Title",
  author       = personList(as.person("Nicholas Potter"))
  journal      = "Journal of ",
  year         = "",
  volume       = "",
  number       = "",
  pages        = "",
  url          = "https://",

  textVersion  =
  paste("Nicholas Potter (2017).",
        "Article Title.",
        "Journal of , vol(no), pg.",
        "URL https://.")
)
